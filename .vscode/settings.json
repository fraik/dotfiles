{
  "docker.containers.label": "ContainerName",
  "editor.cursorStyle": "block",
  "editor.detectIndentation": true,
  "editor.fontFamily": "PragmataPro Mono Liga",
  "editor.fontLigatures": true,
  "editor.fontSize": 16,
  "editor.insertSpaces": true,
  "editor.largeFileOptimizations": false,
  "editor.lineHeight": 17,
  "editor.minimap.enabled": true,
  "editor.renderIndentGuides": false,
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    120,
    80
  ],
  "editor.suggestSelection": "first",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "entity.name.function",
          "entity.name.type",
          "entity.other.inherited-class",
          "storage"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.wordWrap": "on",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.pc": "c",
    "*.sql": "sqltools",
    "*.tpl": "mustache"
  },
  "files.eol": "\n",
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "java.jdt.ls.vmargs": "-noverify -Xmx1G -XX:+UseG1GC -XX:+UseStringDeduplication -javaagent:\"/Users/rubin/.vscode/extensions/gabrielbb.vscode-lombok-1.0.1/server/lombok.jar\"",
  "java.requirements.JDK11Warning": false,
  "java.semanticHighlighting.enabled": false,
  "powershell.sideBar.CommandExplorerVisibility": false,
  "python.disableInstallationCheck": true,
  "python.jediEnabled": false,
  "python.languageServer": "Microsoft",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "PragmataPro Mono Liga",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.fontWeight": "normal",
  "terminal.integrated.fontWeightBold": "normal",
  "terminal.integrated.scrollback": 10000,
  "terminal.integrated.shell.windows": "C:\\Program Files\\PowerShell\\7\\pwsh.exe",
  "window.openFilesInNewWindow": "off",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editorRuler.foreground": "#002855"
  },
  "workbench.colorTheme": "Tomorrow Night Blue",
  "workbench.iconTheme": "vs-minimal",
  "workbench.sideBar.location": "left",
  "workbench.panel.defaultLocation": "right",
  "workbench.startupEditor": "none",
  "[csv]": {
    "editor.tabSize": 24
  },
  "[tsv]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 24
  },
  "[json]": {
    "editor.tabSize": 2
  }
}
